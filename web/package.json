{
  "name": "transmission-web",
  "main": "index.js",
  "repository": "https://github.com/transmission/transmission",
  "license": "MIT",
  "scripts": {
    "dev": "WEBPACK_MODE=development webpack serve --config webpack.config.js",
    "build": "webpack --config webpack.config.js",
    "css": "sass --no-source-map src/assets/css/",
    "css:map": "sass src/assets/css/",
    "lint": "run-p --silent lint:eslint lint:stylelint lint:prettier",
    "lint:fix": "run-s lint:eslint:fix lint:stylelint:fix lint:prettier:fix",
    "lint:eslint": "eslint src/*.js",
    "lint:eslint:fix": "eslint --fix src/*.js",
    "lint:prettier": "prettier --loglevel warn --check package.json src/index.html src/assets/css/*scss src/*.js",
    "lint:prettier:fix": "prettier --loglevel warn -w package.json src/index.html src/assets/css/*scss src/*.js",
    "lint:stylelint": "stylelint src/assets/css/*scss",
    "lint:stylelint:fix": "stylelint --fix src/assets/css/*scss"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.21.1",
    "css-loader": "^6.6.0",
    "cssnano": "^5.0.17",
    "eslint": "^8.10.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-sonarjs": "^0.12.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "file-loader": "^6.2.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.5.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.7",
    "postcss-loader": "^6.2.1",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-scss": "^4.1.0",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.69.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "packageManager": "yarn@3.2.0"
}
